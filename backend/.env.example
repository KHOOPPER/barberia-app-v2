# Base de Datos PostgreSQL
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=your_password
DB_NAME=barberia

# Servidor
PORT=3000
NODE_ENV=development

# Frontend (CORS)
# URL del frontend permitida para realizar peticiones
FRONTEND_URL=http://localhost:5173

# Autenticación JWT
JWT_SECRET=your_jwt_secret_minimum_32_characters
JWT_EXPIRES_IN=7d

# Usuario Administrador Inicial
# Estas credenciales se usarán si no existen usuarios en la DB
ADMIN_USERNAME=admin
ADMIN_PASSWORD=change-this-password
